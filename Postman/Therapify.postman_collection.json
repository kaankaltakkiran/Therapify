          "request": {
            "method": "POST",
            "url": "http://localhost/Therapify/api/admin.php",
            "body": {
              "mode": "raw",
              "raw": "{\n    \"method\": \"get-therapist-applications\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "description": "Get list of all therapist applications"
          }
        },
        {
          "name": "Update Application Status",
          "request": {
            "method": "POST",
            "url": "http://localhost/Therapify/api/admin.php",
            "body": {
              "mode": "raw",
              "raw": "{\n    \"method\": \"update-application-status\",\n    \"application_id\": 1,\n    \"status\": \"approved\",\n    \"admin_notes\": \"Application approved after document verification\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "description": "Update therapist application status (approve/reject)"
