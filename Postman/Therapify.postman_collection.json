    {
      "name": "Authentication",
      "description": "User authentication endpoints",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "url": "http://localhost/Therapify/api/auth.php",
            "body": {
              "mode": "raw",
              "raw": "{\n    \"method\": \"login\",\n    \"email\": \"admin@therapify.com\",\n    \"password\": \"admin123\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "description": "Login with email and password to get JWT token"
          }
        },
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "url": "http://localhost/Therapify/api/auth.php",
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "method",
                  "value": "register",
                  "type": "text"
                },
                {
                  "key": "first_name",
                  "value": "John",
                  "type": "text"
                },
                {
                  "key": "last_name",
                  "value": "Doe",
